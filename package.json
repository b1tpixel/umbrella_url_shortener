{
  "name": "react-backend",
  "version": "0.0.0",
  "private": true,
  "main": "./bin/www",
  "engines": {
    "node": "8.11.3",
    "npm": "5.6.0"
  },
  "scripts": {
    "start": "node ./bin/www",
    "client": "cd client && npm start",
    "dev": "concurrently --kill-others-on-fail \"npm run start\" \"npm run client\"",
    "postinstall": "cd client/ && npm install && ng build"
  },
  "dependencies": {
    "concurrently": "^3.6.1",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "log4js": "^3.0.5",
    "moment": "^2.22.2",
    "morgan": "~1.9.0",
    "node-schedule": "^1.3.0",
    "sequelize": "^4.38.0",
    "sqlite3": "^4.0.2"
  }
}
